      "@tamu-gisc/common/ngx": ["libs/common/ngx/src/index.ts"],
      "@tamu-gisc/common/ngx/pipes": ["libs/common/ngx/pipes/src/index.ts"],
      "@tamu-gisc/common/ngx/local-store": ["libs/common/ngx/local-store/src/index.ts"],
      "@tamu-gisc/common/ngx/settings": ["libs/common/ngx/settings/src/index.ts"],
      "@tamu-gisc/common/ngx/ui": ["libs/common/ngx/ui/src/index.ts"],
      "@tamu-gisc/common/ngx/ditokens": ["libs/common/ngx/ditokens/src/index.ts"],
      "@tamu-gisc/common/ngx/ui/notification": ["libs/common/ngx/ui/notification/src/index.ts"],
      "@tamu-gisc/maps/esri": ["libs/maps/esri/src/index.ts"],
      "@tamu-gisc/common/utils": ["libs/common/utils/src/index.ts"],
      "@tamu-gisc/common/utils/collection": ["libs/common/utils/collection/src/index.ts"],
      "@tamu-gisc/common/utils/database": ["libs/common/utils/database/src/index.ts"],
