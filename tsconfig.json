      "@tamu-gisc/common/utils/geometry": ["libs/common/utils/geometry/src/index.ts"],
      "@tamu-gisc/common/utils/geometry/esri": ["libs/common/utils/geometry/esri/src/index.ts"],
      "@tamu-gisc/common/utils/geometry/generic": ["libs/common/utils/geometry/generic/src/index.ts"],
      "@tamu-gisc/common/utils/number": ["libs/common/utils/number/src/index.ts"],
      "@tamu-gisc/common/utils/object": ["libs/common/utils/object/src/index.ts"],
      "@tamu-gisc/common/utils/string": ["libs/common/utils/string/src/index.ts"],
      "@tamu-gisc/common/utils/routing": ["libs/common/utils/routing/src/index.ts"],
      "@tamu-gisc/common/utils/ui": ["libs/common/utils/ui/src/index.ts"],
