{
  "npmScope": "tamu-gisc",
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "package-lock.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"],
        "parallel": 1
      }
    }
  },
  "affected": {
    "defaultBase": "development"
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "analytics": "b3417ad8-d366-42d6-9c0a-ffba263c7ff4"
  },
  "defaultProject": "aggiemap-angular",
  "generators": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    },
    "@nrwl/angular": {
      "convert-tslint-to-eslint": {
        "skipFormat": false,
        "ignoreExistingTslintConfig": true,
        "removeTSLintIfNoMoreTSLintTargets": true
      }
    },
    "@nrwl/cypress": {
      "convert-tslint-to-eslint": {
        "skipFormat": false,
        "ignoreExistingTslintConfig": true,
        "removeTSLintIfNoMoreTSLintTargets": true
      }
    }
  }
}
